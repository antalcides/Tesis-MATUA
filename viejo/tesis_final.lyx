\hline
$\boldsymbol{F}_{1}^{\top}$ & $\cdots$ & $\boldsymbol{F}_{b}^{\top}\boldsymbol{1}_{m}$ & $\displaystyle\sum_{i=1}^{b}\boldsymbol{F}_{i}^{\top}\boldsymbol{F}_{i}$\\
\end{tabular}\right)
\end{equation}
\end_inset


\begin_inset Newline newline
\end_inset

 
\begin_inset Newline newline
\end_inset

 Para 
\begin_inset Formula $\boldsymbol{\beta}$
\end_inset

